<svg
  id={{elementId}}
  class="ui-knob-arc"
  width={{width}}
  height={{height}}
>
  <g
    class='arcs'
    transform="translate({{transformSize}},{{transformSize}}){{if _flip ' scale(-1, -1)'}}"
  >
    <path
      d={{arc}}
      class='unselected'
      style="stroke: {{unselectedColor}}; fill: {{unselectedColor}};"
    ></path>
    <path
      class='selected'
      d={{selectedArc}}
      style="stroke: {{selectedColor}}; fill: {{selectedColor}};"
    ></path>
  </g>

</svg>
